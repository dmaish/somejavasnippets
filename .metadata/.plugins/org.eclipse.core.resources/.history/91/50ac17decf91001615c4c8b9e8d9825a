package characterClass;

public class someClass {
	public static void main(String[]args){
		String someValue="abc123";
		
		//looping through all the characters in the someValue String
		for(int i=0;i<someValue.length();i++){
			char someIndex=someValue.charAt(i);
			
			//see if the character is a letter or not
			if(Character.isLetter(someIndex)){
				System.out.println(someIndex + "=Letter");
			}else{
				System.out.println(someIndex);
			}
			
		}
	}
}
